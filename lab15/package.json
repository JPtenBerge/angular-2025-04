{
  "name": "datepicker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:ng\" \"npm run start:server\"",
    "start:ng": "ng serve",
    "start:server": "json-server --watch ./date-picker.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "npm run test:karma",
    "test:ui": "playwright test",
    "pretest:karma": "node tasks/switch-test-runner.mjs karma",
    "test:karma": "ng test --configuration karma --exclude \"src/**/*.jest.spec.ts\"",
    "pretest:jest": "node tasks/switch-test-runner.mjs jest",
    "test:jest": "ng test --configuration jest --exclude \"**/*.karma.spec.ts\" --exclude \"e2e/**/*.ts\"",
    "test:jest:build": "npm run test:jest || exit 0",
    "lint": "ng lint",
    "lint:ui": "tsc -p e2e",
    "pretest:mutation:jest": "node tasks/switch-test-runner.mjs jest",
    "pretest:mutation:karma": "node tasks/switch-test-runner.mjs karma",
    "test:mutation:jest": "stryker run stryker.jest.config.json",
    "test:mutation:karma": "stryker run stryker.karma.config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.3",
    "@angular/common": "^17.1.3",
    "@angular/compiler": "^17.1.3",
    "@angular/core": "^17.1.3",
    "@angular/forms": "^17.1.3",
    "@angular/platform-browser": "^17.1.3",
    "@angular/platform-browser-dynamic": "^17.1.3",
    "@angular/router": "^17.1.3",
    "bootstrap": "^5.3.2",
    "ngx-toastr": "^18.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.3",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.1.3",
    "@angular/compiler-cli": "^17.1.3",
    "@playwright/test": "^1.41.2",
    "@stryker-mutator/core": "^8.2.3",
    "@stryker-mutator/jest-runner": "^8.2.3",
    "@stryker-mutator/karma-runner": "^8.2.3",
    "@types/jasmine": "~5.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-server": "^0.17.4",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.3"
  }
}
